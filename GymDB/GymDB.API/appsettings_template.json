{
  "ConnectionStrings": {
    "PostgresConnection": "Server=<server>;Database=<db_name>;Port=<port>;User Id=<user_id>;Password=<password>;"
  },
  "AzureSettings": {
    "StorageAccount": "<azure_storage_account>",
    "AccessKey": "<azure_storage_account_access_key>",
    "ExerciseImageContainer": "<azure_container>",
    "ExerciseVideoContainer": "<azure_container>"
  },
  "JwtSettings": {
    "Issuer": "<issuer>",
    "Audience": "<audience>",
    "SecretKey": "<jwt_secret_key>",
    "TokenLifetime": "00:00:00:00"
  },
  "SessionSettings": {
    "SessionLifetime": "00:00:00:00"
  },
  "DBSeed": {
    "Roles": {
      "Super Admin": "<color>",
      "<role_name_1>": "<color>",
      "<role_name_2>": "<color>"
    },
    "DefaultRole": "<role_name>",
    "RootAdmin": {
      "Username": "<root_username>",
      "Email": "<root_email>",
      "Password": "<root_password>"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
  